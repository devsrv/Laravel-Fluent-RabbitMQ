{
    "name": "sokanacademy/laravel-fluent-rabbitmq",
    "description": "A package to work with RabbitMQ in an elegant way.",
    "keywords": [
        "sokanacademy",
        "laravel",
        "laravel-fluent-rabbitmq"
    ],
    "homepage": "https://github.com/sokanacademy/laravel-fluent-rabbitmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Sokanacademy",
            "email": "sokanacademy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Sokanacademy\\LaravelFluentRabbitMQ\\": "src",
            "Sokanacademy\\LaravelFluentRabbitMQ\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sokanacademy\\LaravelFluentRabbitMQ\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sokanacademy\\LaravelFluentRabbitMQ\\LaravelFluentRabbitMQServiceProvider"
            ],
            "aliases": {
                "LaravelFluentRabbitMQ": "Sokanacademy\\LaravelFluentRabbitMQ\\Facades\\LaravelFluentRabbitMQ"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}